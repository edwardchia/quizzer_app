
<div class="card">
<div class="card-body">
  <%= form_with url: session_path, local: true do |form| %>
      <div class="form-group">
        <%= form.label :email %>
        <%= form.email_field :email, class: "form-control", placeholder: "Enter Email" %>
      
      </div>
      <div class="form-group">
        <%= form.label :password %>
        <%= form.password_field :password, class: "form-control", placeholder: "Password" %>
        
      </div>
      <%= form.submit "Sign In", :class=> "btn btn-primary" %>
    <% end %>
    </div>
    </div>
  
